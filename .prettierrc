{
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "printWidth": 120,
  "plugins": [
    "prettier-plugin-go-template"
  ],
  "overrides": [
    {
      "files": [ "*.html"],
      "options": {"parser": "go-template"}
    }
  ]
}
